#!/usr/bin/env bash

# To add debugger
# set -x

# Source my color library

source ru_var_func

# Home variables

MY_EXE="$HOME/git_repos/bin"

for script in "$@"; do
	cp $script $MY_EXE 2>> $ERROR_LOG 
	if [ $? -eq 0 ]; then
		echo -e "${GREEN}SUCCESS${RESET}: Moved $script to $MY_EXE"
	else
		echo -e "${RED}ERROR${RESET}: Could not locate $script in $(pwd)"
	fi
done
